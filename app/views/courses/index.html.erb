<nav class="dashboard-nav">
  <ul>
    <li><%= link_to "Logout", logout_path, method: :delete %></li>
  </ul>
</nav>
<div class="wrapper">

<h1>Student Dashboard</h1>

<table class="table table-bordered">
  <thead>
  <tr>
    <th>Course Name</th>
    <th>Description</th>
    <th>Date Started</th>
    <th>Date End</th>
  </tr>
</thead>
<% @courses.each do |course| %>
<tbody>
  <tr>
    <td> <%= link_to course.name, contents_path	, class: 'view-content-link'%></td>
    <td><%= course.description %></td>
    <td><%= course.date_started %></td>
    <td><%= course.date_ended %></td>
  </tr>
<% end %>
</tbody>
</table>

</div>
